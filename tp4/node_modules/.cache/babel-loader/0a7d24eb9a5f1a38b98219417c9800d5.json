{"ast":null,"code":"//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\nexport default {\n  name: \"src-components-formulario\",\n  props: [],\n  mounted: function mounted() {},\n  data: function data() {\n    return {\n      formData: this.getInicialData(),\n      formState: {},\n      nombreMinLength: 3,\n      edadMin: 18,\n      edadMax: 40\n    };\n  },\n  methods: {},\n  computed: {}\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA8HA;EACAA,iCADA;EAEAC,SAFA;EAGAC,OAHA,qBAGA,EAHA;EAIAC,IAJA,kBAIA;IACA;MACAC,+BADA;MAEAC,aAFA;MAGAC,kBAHA;MAIAC,WAJA;MAKAC;IALA;EAOA,CAZA;EAaAC,WAbA;EAcAC;AAdA","names":["name","props","mounted","data","formData","formState","nombreMinLength","edadMin","edadMax","methods","computed"],"sourceRoot":"src/components","sources":["Formulario.vue"],"sourcesContent":["<template>\n  <section class=\"src-components-formulario\">\n    <vue-form :state=\"formState\" @submit.prevent=\"enviar()\">\n      <!-- Campo nombre -->\n      <validate tag=\"div\">\n        <!-- elemento de entrada -->\n        <label for=\"nombre\">Nombre</label>\n        <input\n          type=\"text\"\n          id=\"nombre\"\n          name=\"nombre\"\n          class=\"form-control\"\n          v-model.trim=\"formData.nombre\"\n          autocomplete=\"off\"\n          required\n          :minlength=\"nombreMinLength\"\n          no-espacios\n        />\n\n        <!-- Mensajes de validacion -->\n        <field-messages name=\"nombre\" show=\"$dirty\">\n          <div slot=\"required\" class=\"alert alert-danger mt-1\">\n            Campo requerido\n          </div>\n          <div slot=\"minlength\" class=\"alert alert-danger mt-1\">\n            Este campo requiere como minumo {{ nombreMinLength }} caracteres\n          </div>\n          <div slot=\"no-espacios\" class=\"alert alert-danger mt-1\">\n            No se permite espacios intermedios en este campo\n          </div>\n        </field-messages>\n      </validate>\n\n      <!-- Campo apellido -->\n      <validate tag=\"div\">\n        <!-- elemento de entrada -->\n        <label for=\"apellido\">Apellido</label>\n        <input\n          type=\"text\"\n          id=\"apellido\"\n          name=\"apellido\"\n          class=\"form-control\"\n          v-model.trim=\"formData.apellido\"\n          autocomplete=\"off\"\n          required\n        />\n\n        <!-- Mensajes de validacion -->\n        <field-messages name=\"apellido\" show=\"$dirty\">\n          <div slot=\"required\" class=\"alert alert-danger mt-1\">\n            Campo requerido\n          </div>\n        </field-messages>\n      </validate>\n\n      <!-- Campo edad -->\n      <validate tag=\"div\">\n        <!-- elemento de entrada -->\n        <label for=\"edad\">edad</label>\n        <input\n          type=\"number\"\n          id=\"edad\"\n          name=\"edad\"\n          class=\"form-control\"\n          v-model.trim=\"formData.edad\"\n          autocomplete=\"off\"\n          required\n          :min=\"edadMin\"\n          :max=\"edadMax\"\n        />\n\n        <!-- Mensajes de validacion -->\n        <field-messages name=\"edad\" show=\"$dirty\">\n          <div slot=\"required\" class=\"alert alert-danger mt-1\">\n            Campo requerido\n          </div>\n          <div slot=\"minlength\" class=\"alert alert-danger mt-1\">\n            Este campo requiere como minumo {{ nombreMinLength }} caracteres\n          </div>\n          <div slot=\"min\" class=\"alert alert-danger mt-1\">\n            La edad minima permitida es de {{ edadMin }}\n          </div>\n          <div slot=\"max\" class=\"alert alert-danger mt-1\">\n            La edad minima permitida es de {{ edadMax }}\n          </div>\n        </field-messages>\n      </validate>\n\n      <!-- Campo email -->\n      <validate tag=\"div\">\n        <!-- elemento de entrada -->\n        <label for=\"email\">Email</label>\n        <input\n          type=\"email\"\n          id=\"email\"\n          name=\"email\"\n          class=\"form-control\"\n          v-model.trim=\"formData.email\"\n          autocomplete=\"off\"\n          required\n        />\n\n        <!-- Mensajes de validacion -->\n        <field-messages name=\"email\" show=\"$dirty\">\n          <div slot=\"required\" class=\"alert alert-danger mt-1\">\n            Campo requerido\n          </div>\n          <div slot=\"email\" class=\"alert alert-danger mt-1\">\n            Email no valido\n          </div>\n        </field-messages>\n      </validate>\n\n      <!-- boton de envio -->\n      <button\n        type=\"submit\"\n        class=\"btn btn-success my-3\"\n        :disabled=\"formState.$invalid\"\n      >\n        Enviar\n      </button>\n    </vue-form>\n  </section>\n</template>\n\n<script>\nexport default {\n  name: \"src-components-formulario\",\n  props: [],\n  mounted() {},\n  data() {\n    return {\n      formData: this.getInicialData(),\n      formState: {},\n      nombreMinLength: 3,\n      edadMin: 18,\n      edadMax: 40,\n    };\n  },\n  methods: {},\n  computed: {},\n};\n</script>\n\n<style scoped lang=\"css\"></style>\n"]},"metadata":{},"sourceType":"module"}